.title{
  margin-top: 50rpx;
}
.container1{
  border: 1rpx solid;
  display: flex;
  justify-content: space-evenly;
}

.container1 view{
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
}

.hoverclass{
  box-shadow: 0 0 15rpx red;
}

.container2{
  width: 100%;
  height: 300rpx;
  border: 1rpx solid;
  /* 不允许换行 */
  white-space: nowrap;
}

.container3{
  width: 100%;
  height: 300rpx;
  border: 1rpx solid;
}

.scrollItem{
  width: 100%;
  height: 300rpx;
  color: #fff;
  font-size: 100rpx;
  text-align: center;
  line-height: 300rpx;
  display: inline-block;
}

.image{
  width: 200rpx;
  height: 200rpx;
  background-color: #eee;
}

.navigator{
  width: 200rpx;
}

progress{
  margin-bottom: 20rpx;
}

